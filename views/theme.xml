<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <template id="clean_script" inherit_id="website.assets_frontend" name="website_multi_image assets">
            <xpath expr="//link[last()]" position="after">
                <link href="/website_multi_image/static/lib/slick/slick.css" rel="stylesheet" type="text/css"/>
                <link href="/website_multi_image/static/lib/slick/slick-theme.css" rel="stylesheet" type="text/css"/>
                <link href="/website_multi_image/static/src/css/website-image-carousel.css" rel="stylesheet" type="text/css"/>
            </xpath>
            <xpath expr="//script[last()]" position="after">
                <script type="text/javascript" src="/website_multi_image/static/lib/slick/slick.min.js" data-no-instant=""></script>
                <script type="text/javascript" src="/website_multi_image/static/src/js/website-image-carousel.js" data-no-instant=""></script>
            </xpath>
        </template>
    </data>
</openerp>
